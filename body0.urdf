            <geometry>
                <box size="1.5 1.5 1" />
            </geometry>
            <material name="Green">
                <color rgba="0.0 1.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.75 0" rpy="0 0 0"/>
            <geometry>
                <box size="1.5 1.5 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Part1_Part2" type="revolute">
       <parent link="Part1"/>
       <child  link="Part2"/>
