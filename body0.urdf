            <geometry>
                <box size="2.0 2.5 1" />
            </geometry>
            <material name="Blue">
                <color rgba="0.0 0.5 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 1.25 0" rpy="0 0 0"/>
            <geometry>
                <box size="2.0 2.5 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Part2_Part3" type="revolute">
       <parent link="Part2"/>
