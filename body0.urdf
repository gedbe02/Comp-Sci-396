        <visual>
            <origin xyz="0 1.75 0" rpy="0 0 0"/>
            <geometry>
                <box size="2.5 3.5 1" />
            </geometry>
            <material name="Blue">
                <color rgba="0.0 0.5 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 1.75 0" rpy="0 0 0"/>
            <geometry>
